{
  "name": "nextplate",
  "version": "1.0.0",
  "description": "",
  "main": "next",
  "author": "Lucas Martin <contact@lucasmartin.fr> (http://lucasmartin.fr/)",
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "start": "next start",
    "build": "npm run clear && next build",
    "clear": "rm -rf dist/ && rm -rf .next/ && rm -rf node_modules/.cache",
    "export": "npm run build && next export -o dist",
    "lint": "tsc --noEmit && eslint --color src/**/*.{ts,tsx}",
    "lint:fix": "npm run lint -- --fix"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "lint-staged": {
    "src/**/*.{ts,tsx}": "npm run lint:fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "license": "UNLICENSED",
  "dependencies": {
    "@zeit/next-css": "1.0.1",
    "lazysizes": "^5.2.2",
    "next": "^9.5.5",
    "next-seo": "4.7.3",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "styled-components": "5.1.1",
    "tailwindcss": "^1.9.1",
    "tailwindcss-aspect-ratio": "^3.0.0",
    "twin.macro": "^1.10.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "9.5.2",
    "@types/node": "14.0.27",
    "@types/react": "16.9.46",
    "@types/react-dom": "16.9.8",
    "@types/styled-components": "^5.1.2",
    "@typescript-eslint/eslint-plugin": "3.9.0",
    "@typescript-eslint/parser": "3.9.0",
    "babel-eslint": "10.1.0",
    "babel-plugin-graphql-tag": "3.0.0",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-styled-components": "1.11.1",
    "eslint": "7.7.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-react-app": "5.2.1",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.6",
    "eslint-plugin-react-hooks": "4.1.0",
    "husky": "4.2.5",
    "lint-staged": "10.2.11",
    "prettier": "2.0.5",
    "typescript": "3.9.7"
  }
}
