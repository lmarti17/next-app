{
  "name": "nextplate",
  "version": "1.0.0",
  "description": "",
  "main": "next",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next build && next export -o export",
    "deploy": "now --prod",
    "clean-babel-cache": "rm -Rf ./node_modules/.cache/babel-loader/*"
  },
  "keywords": [],
  "author": "Lucas Martin <contact@lucasmartin.fr> (http://lucasmartin.fr/)",
  "license": "UNLICENSED",
  "dependencies": {
    "next": "^9.3.6",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "@zeit/next-sass": "^1.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^3.1.3",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "^1.19.1",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "url-loader": "^1.1.2",
    "stylelint": "^13.3.3",
    "stylelint-config-standard": "^20.0.0",
    "eslint-plugin-import": "^2.20.2",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2"
  },
  "engines": {
    "node": "12.x"
  },
  "lint-staged": {
    "*.{js,mjs}": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.scss": "stylelint --config ./.stylelintrc.json --syntax=scss --fix "
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
